{{#partial "block-content"}}
<div class="background--gallery">
    <div class="wrapper">
        <div class="tiles">
            <h2>Latest news, figures and releases</h2>
            <div class="col-wrap tiles__block tiles__block-no-mar-bottom">
                <div class="col col--md-23 col--lg-14 height-sm--48 height-md--48 height-lg--48 background--white margin-left-md--1 margin-bottom--2 js-hover-click">
                    {{>partials/t1/home-figure sections.0 ratio="1:2"}}
