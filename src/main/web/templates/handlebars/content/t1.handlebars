                        <div class="box__content box__content--homepage">
                            <h2 class="tiles__title-h2--home"><a href="/timeseriestool">Looking to download time series?</a></h2>
                            <p class="margin-top--1">Try our time series explorer</p>
                        </div>
                    </div>
                </div>
                <div class="col col--md-23 col--lg-29 height-sm--48 height-md--23 background--white margin-left-md--1 margin-bottom--2 js-hover-click">
                    <article class="">

                        <div class="">
                            {{#resolve sections.2 filter="title"}}
                            {{!-- <h2 class="tiles__title-h2"><a href="{{uri}}">{{title}}</a></h2> --}}
                            {{!-- <h2 class="tiles__title-h2">{{title}}</h2> --}}
                            {{/resolve}}

                            {{#resolve sections.2.statistics.uri}}

                                <dl class="box__content box__content--homepage height-md--23 padding-top-sm--24 padding-right-md--1 padding-bottom-md--0 padding-left-md--11 padding-left-lg--14">
                                    <dt class="tiles__title-dt margin-left-sm--1 margin-left-md--1">
                                        <a href="{{uri}}">{{description.title}}</a>
                                    </dt>
                                    <!-- change to dd's -->
                                    <dd class="margin-left-sm--1 margin-left-md--1">
                                        <span class="tiles__preunit--home">{{description.preUnit}}</span>
                                        <span class="tiles__figure--home stat__figure-enhance">{{description.number}}</span>
                                        <span class="tiles__unit--home">{{description.unit}}</span>
                                        </span>
                                        <span class="tiles__date--home">
                                            {{description.date}}
                                        </span>
                                        <a class="js-enhance--hide" href="{{statistics.uri}}">More info</a>
                                        <div class="sparkline-holder width-md--11 width-lg--14">
                                            {{> partials/highcharts/sparkline}}
                                        </div>
                                    </dd>
                                </dl>
                            {{/resolve}}
                        </div>
                    </article>
                    {{!-- {{>partials/t1/home-figure sections.2 ratio="2:1"}} --}}
                </div>
                <div class="col col--md-11 col--lg-14 height-sm--23 height-md--23 background--white margin-left-md--1 margin-bottom--2 js-hover-click">
